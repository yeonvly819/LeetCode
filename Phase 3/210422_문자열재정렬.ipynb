    "S = input()\n",
    "\n",
    "w = ''\n",
    "n = 0\n",
    "\n",
    "for i in S:\n",
    "    try:\n",
    "        if int(i):\n",
    "            n += int(i)\n",
    "    except:\n",
    "        w += i\n",
    "\n",
    "w = ''.join(sorted(list(w), reverse=False))\n",
    "\n",
    "print(w + str(n))"
